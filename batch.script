#!/bin/sh
#SBATCH -J NATIVE_BARRIER_BUTTERFLY
#SBATCH -o NATIVE_BARRIER_BUTTERFLY.%J.stdout
#SBATCH -e NATIVE_BARRIER_BUTTERFLY.%J.stderr
#SBATCH -p development
